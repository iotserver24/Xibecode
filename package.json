{
  "name": "xibecode",
  "version": "0.5.7",
  "description": "AI-powered autonomous coding assistant with WebUI, AI test generation, multi-model support, and plugin system",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "xibecode": "./dist/index.js",
    "xc": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:webui": "cd webui && pnpm install && pnpm build",
    "build:all": "npm run build && npm run build:webui",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "ui": "tsx src/index.ts ui --open",
    "ui:dev": "tsx watch src/index.ts ui",
    "docs:generate": "typedoc --options typedoc.json",
    "docs:watch": "typedoc --options typedoc.json --watch",
    "docs:serve": "npx http-server ./docs/api-generated -p 8080 -o",
    "postinstall": "npx playwright install chromium || echo \"Playwright install failed, skipping optional step...\""
  },
  "keywords": [
    "cli",
    "ai",
    "coding",
    "assistant",
    "autonomous",
    "agent",
    "automation",
    "webui",
    "test-generation"
  ],
  "author": "Anish Kumar (R3AP3R editz) <https://github.com/iotserver24>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/iotserver24/xibecode"
  },
  "bugs": {
    "url": "https://github.com/iotserver24/xibecode/issues"
  },
  "homepage": "https://github.com/iotserver24/xibecode#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "conf": "^15.1.0",
    "diff": "^5.1.0",
    "dotenv": "^16.3.1",
    "fast-glob": "^3.3.2",
    "ink": "^6.7.0",
    "ink-markdown": "^1.0.4",
    "ink-select-input": "^6.2.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^8.2.7",
    "node-fetch": "^3.3.2",
    "open": "^10.0.0",
    "ora": "^5.4.1",
    "playwright": "^1.41.2",
    "react": "^19.2.4",
    "strip-ansi": "^7.1.0",
    "ts-morph": "^27.0.2",
    "ws": "^8.16.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/diff": "^5.0.8",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^25.3.3",
    "@types/ws": "^8.5.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "webui-dist",
    "README.md"
  ]
}